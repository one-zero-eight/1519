  "components": {
    "schemas": {
      "AddPatronRequest": {
        "properties": {
          "telegram_id": {
            "type": "string",
            "title": "Telegram Id",
            "description": "Telegram ID of the patron"
          },
          "telegram_data": {
            "type": "object",
            "title": "Telegram Data",
            "description": "Data from Telegram Login Widget"
          },
          "is_admin": {
            "type": "boolean",
            "title": "Is Admin",
            "description": "Is the patron an admin",
            "default": false
          }
        },
        "type": "object",
        "required": ["telegram_id", "telegram_data"],
        "title": "AddPatronRequest"
      },
      "ApplicationRankingStats": {
        "properties": {
          "application": {
            "$ref": "#/components/schemas/ApplicationResponse"
          },
          "rrf_score": {
            "type": "number",
            "title": "Rrf Score"
          },
          "positive_votes": {
            "type": "integer",
            "title": "Positive Votes"
          },
          "negative_votes": {
            "type": "integer",
            "title": "Negative Votes"
          },
          "neutral_votes": {
            "type": "integer",
            "title": "Neutral Votes"
          },
          "total_votes": {
            "type": "integer",
            "title": "Total Votes"
          }
        },
        "type": "object",
        "required": [
          "application",
          "rrf_score",
          "positive_votes",
          "negative_votes",
          "neutral_votes",
          "total_votes"
        ],
        "title": "ApplicationRankingStats"
      },
      "ApplicationResponse": {
        "properties": {
          "id": {
            "type": "integer",
            "title": "Id"
          },
          "submitted_at": {
            "type": "string",
            "format": "date-time",
            "title": "Submitted At",
            "description": "Time when the application was submitted"
          },
          "session_id": {
            "type": "string",
            "title": "Session Id",
            "description": "Session ID of user who created the application"
          },
          "email": {
            "type": "string",
            "title": "Email",
            "description": "Innopolis email of the participant"
          },
          "full_name": {
            "type": "string",
            "title": "Full Name",
            "description": "Full name of the participant"
          },
          "cv": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Cv",
            "description": "Path to the CV of the participant"
          },
          "motivational_letter": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Motivational Letter",
            "description": "Path to the motivational letter of the participant"
          },
          "recommendation_letter": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Recommendation Letter",
            "description": "Path to the recommendation letter of the participant"
          },
          "transcript": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Transcript",
            "description": "Path to the transcript of the participant"
          },
          "almost_a_student": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Almost A Student",
            "description": "Path to the \"Almost A student\" document of the participant"
          }
        },
        "type": "object",
        "required": [
          "id",
          "submitted_at",
          "session_id",
          "email",
          "full_name",
          "cv",
          "motivational_letter",
          "recommendation_letter",
          "transcript",
          "almost_a_student"
        ],
        "title": "ApplicationResponse"
      },
      "Body_put_ranking_route_patron_ranking_put": {
        "properties": {
          "application_ids": {
            "items": {
              "type": "integer"
            },
            "type": "array",
            "title": "Application Ids"
          }
        },
        "type": "object",
        "required": ["application_ids"],
        "title": "Body_put_ranking_route_patron_ranking_put"
      },
      "DailyApplicationStats": {
        "properties": {
          "date": {
            "type": "string",
            "format": "date",
            "title": "Date"
          },
          "applications_received": {
            "type": "integer",
            "title": "Applications Received"
          }
        },
        "type": "object",
        "required": ["date", "applications_received"],
        "title": "DailyApplicationStats"
      },
      "DailyPatronStats": {
        "properties": {
          "date": {
            "type": "string",
            "format": "date",
            "title": "Date"
          },
          "rating_count": {
            "type": "integer",
            "title": "Rating Count"
          },
          "ranking_count": {
            "type": "integer",
            "title": "Ranking Count"
          }
        },
        "type": "object",
        "required": ["date", "rating_count", "ranking_count"],
        "title": "DailyPatronStats"
      },
      "Docs": {
        "properties": {
          "cv_comments": {
            "type": "string",
            "title": "Cv Comments",
            "default": ""
          },
          "cv_seen": {
            "type": "boolean",
            "title": "Cv Seen",
            "default": false
          },
          "motivational_letter_comments": {
            "type": "string",
            "title": "Motivational Letter Comments",
            "default": ""
          },
          "motivational_letter_seen": {
            "type": "boolean",
            "title": "Motivational Letter Seen",
            "default": false
          },
          "recommendation_letter_comments": {
            "type": "string",
            "title": "Recommendation Letter Comments",
            "default": ""
          },
          "recommendation_letter_seen": {
            "type": "boolean",
            "title": "Recommendation Letter Seen",
            "default": false
          },
          "transcript_comments": {
            "type": "string",
            "title": "Transcript Comments",
            "default": ""
          },
          "transcript_seen": {
            "type": "boolean",
            "title": "Transcript Seen",
            "default": false
          },
          "almost_a_student_comments": {
            "type": "string",
            "title": "Almost A Student Comments",
            "default": ""
          },
          "almost_a_student_seen": {
            "type": "boolean",
            "title": "Almost A Student Seen",
            "default": false
          }
        },
        "type": "object",
        "title": "Docs",
        "description": "Comments and seen flags for patron process of application"
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "OverallStats": {
        "properties": {
          "total_patrons": {
            "type": "integer",
            "title": "Total Patrons"
          },
          "total_applications": {
            "type": "integer",
            "title": "Total Applications"
          },
          "patron_activity_by_day": {
            "items": {
              "$ref": "#/components/schemas/DailyPatronStats"
            },
            "type": "array",
            "title": "Patron Activity By Day"
          },
          "applications_by_day": {
            "items": {
              "$ref": "#/components/schemas/DailyApplicationStats"
            },
            "type": "array",
            "title": "Applications By Day"
          }
        },
        "type": "object",
        "required": [
          "total_patrons",
          "total_applications",
          "patron_activity_by_day",
          "applications_by_day"
        ],
        "title": "OverallStats"
      },
      "PatronRankingResponse": {
        "properties": {
          "patron_id": {
            "type": "integer",
            "title": "Patron Id",
            "description": "Who ranked"
          },
          "applications": {
            "items": {
              "$ref": "#/components/schemas/ApplicationResponse"
            },
            "type": "array",
            "title": "Applications",
            "description": "Who was ranked, in order"
          }
        },
        "type": "object",
        "required": ["patron_id", "applications"],
        "title": "PatronRankingResponse"
      },
      "PatronRateApplicationResponse": {
        "properties": {
          "patron_id": {
            "type": "integer",
            "title": "Patron Id",
            "description": "Who rated"
          },
          "application_id": {
            "type": "integer",
            "title": "Application Id",
            "description": "Who was rated"
          },
          "comment": {
            "type": "string",
            "title": "Comment",
            "description": "Comment for whole application",
            "default": ""
          },
          "docs": {
            "$ref": "#/components/schemas/Docs",
            "description": "Comments and seen flags for documents",
            "default": {
              "cv_comments": "",
              "cv_seen": false,
              "motivational_letter_comments": "",
              "motivational_letter_seen": false,
              "recommendation_letter_comments": "",
              "recommendation_letter_seen": false,
              "transcript_comments": "",
              "transcript_seen": false,
              "almost_a_student_comments": "",
              "almost_a_student_seen": false
            }
          },
          "rate": {
            "type": "integer",
            "title": "Rate",
            "description": "Rating value"
          }
        },
        "type": "object",
        "required": ["patron_id", "application_id", "rate"],
        "title": "PatronRateApplicationResponse"
      },
      "PatronResponse": {
        "properties": {
          "telegram_id": {
            "type": "string",
            "title": "Telegram Id",
            "description": "Telegram ID of the patron"
          },
          "telegram_data": {
            "type": "object",
            "title": "Telegram Data",
            "description": "Data from Telegram Login Widget"
          },
          "is_admin": {
            "type": "boolean",
            "title": "Is Admin",
            "description": "Is the patron an admin"
          }
        },
        "type": "object",
        "required": ["telegram_id", "telegram_data", "is_admin"],
        "title": "PatronResponse"
      },
      "PatronStats": {
        "properties": {
          "patron_id": {
            "type": "integer",
            "title": "Patron Id"
          },
          "total_ratings": {
            "type": "integer",
            "title": "Total Ratings"
          },
          "activity_by_day": {
            "items": {
              "$ref": "#/components/schemas/DailyPatronStats"
            },
            "type": "array",
            "title": "Activity By Day"
          }
        },
        "type": "object",
        "required": ["patron_id", "total_ratings", "activity_by_day"],
        "title": "PatronStats"
      },
      "PatronWithRatingsAndRankings": {
        "properties": {
          "patron": {
            "$ref": "#/components/schemas/PatronResponse"
          },
          "ratings": {
            "items": {
              "$ref": "#/components/schemas/PatronRateApplicationResponse"
            },
            "type": "array",
            "title": "Ratings"
          },
          "ranking": {
            "$ref": "#/components/schemas/PatronRankingResponse"
          }
        },
        "type": "object",
        "required": ["patron", "ratings", "ranking"],
        "title": "PatronWithRatingsAndRankings"
      },
      "SubmitForm": {
        "properties": {
          "email": {
            "type": "string",
            "minLength": 1,
            "pattern": "^[a-zA-Z0-9_.+-]+@(innopolis\\.(university|ru))$",
            "title": "Email",
            "description": "Innopolis email of the participant (...@innopolis.university or ...@innopolis.ru)",
            "examples": ["i.ivanov@innopolis.university"]
          },
          "full_name": {
            "type": "string",
            "minLength": 1,
            "title": "Full Name",
            "description": "Full name of the participant",
            "examples": ["Ivan Ivanov"]
          },
          "cv_file": {
            "type": "string",
            "format": "binary",
            "title": "Cv File",
            "description": "CV of the participant"
          },
          "transcript_file": {
            "type": "string",
            "format": "binary",
            "title": "Transcript File",
            "description": "Transcript of the participant"
          },
          "motivational_letter_file": {
            "type": "string",
            "format": "binary",
            "title": "Motivational Letter File",
            "description": "Motivational letter of the participant"
          },
          "recommendation_letter_file": {
            "type": "string",
            "format": "binary",
            "title": "Recommendation Letter File",
            "description": "Recommendation letter of the participant"
          },
          "almost_a_student_file": {
            "type": "string",
            "format": "binary",
            "title": "Almost A Student File",
            "description": "\"Almost A student\" document of the participant"
          }
        },
        "type": "object",
        "required": ["email", "full_name"],
        "title": "SubmitForm"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "type": "array",
            "title": "Location"
          },
          "msg": {
            "type": "string",
            "title": "Message"
          },
          "type": {
            "type": "string",
            "title": "Error Type"
          }
        },
        "type": "object",
        "required": ["loc", "msg", "type"],
        "title": "ValidationError"
      }
    }
  }
