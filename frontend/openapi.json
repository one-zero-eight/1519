          },
          {
            "name": "comment",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "default": "",
              "title": "Comment"
            }
          },
          {
            "name": "rate",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer",
              "default": 0,
              "title": "Rate"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Docs",
                "default": {
                  "cv_comments": "",
                  "cv_seen": false,
                  "motivational_letter_comments": "",
                  "motivational_letter_seen": false,
                  "recommendation_letter_comments": "",
                  "recommendation_letter_seen": false,
                  "transcript_comments": "",
                  "transcript_seen": false,
                  "almost_a_student_comments": "",
                  "almost_a_student_seen": false
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful Response",
